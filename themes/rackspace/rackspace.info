name = Rackspace
description = A custom theme built for Rackspace by David Stinemetze.
core = 7.x

;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[ceiling_left]   = 'Ceiling - Left'
regions[ceiling_right]  = 'Ceiling - Right'
regions[highlighted]    = 'Highlighted'
regions[help]           = 'Help'
regions[hero]           = 'Hero'
regions[content]        = 'Content'
regions[carpet]         = 'Carpet'
regions[footer]         = 'Footer'
regions[basement]       = 'Basement'

;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = bootstrap/css/bootstrap.min.css
stylesheets[all][] = css/style.css

;;;;;;;;;;;;;;;;;;;;;
;; Scripts
;;;;;;;;;;;;;;;;;;;;;

scripts[] = bootstrap/js/bootstrap.min.js
scripts[] = bootstrap/js/plugins/collapse.js
scripts[] = js/jquery.rackspace.js
scripts[] = js/global.js
scripts[] = js/global.header.js
scripts[] = js/global.live-chat.js
